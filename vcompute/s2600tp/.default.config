#
# Automatically generated make config: don't edit
#

#
# Embedded Filesystem
#
CONFIG_ROOTFS=y
CONFIG_HOSTNAME="vBMC1.0"
CONFIG_EXTRA_FILES=""
# CONFIG_NFS is not set
CONFIG_NFS_REMOTE=""
CONFIG_NFS_MNT_PNT=""

#
# Build Options
#
CONFIG_CC_JOBS="4"
# CONFIG_USE_DISTCC is not set
CONFIG_ARCH="x86_64"
CONFIG_CC="gcc"
CONFIG_FLAGS=""

#
# Linux Kernel Setup
#
CONFIG_PKG_kernel=y
CONFIG_KERNEL_CC="/usr/bin/gcc"
CONFIG_KERNEL_VERSION="3.16"

#
# Libraries
#
CONFIG_PKG_libpkg=y

#
# libcurses
#
CONFIG_PKG_libncurses=y
CONFIG_USE_LIBNCURSES_DEB_PKG=y
CONFIG_LIBNCURSES_DEB_URL="http://mirrors.kernel.org/ubuntu/pool/main/n/ncurses/"

#
# libapr
#
CONFIG_PKG_libapr=y
CONFIG_USE_LIBAPR_DEB_PKG=y
CONFIG_LIBAPR_DEB_URL="http://security.ubuntu.com/ubuntu/pool/main/a/apr/"

#
# httplib2
#
CONFIG_PKG_httplib2=y
CONFIG_PKG_openssh=y
CONFIG_PKG_busybox=y

#
# Driver Packages
#
# CONFIG_PKG_be2net is not set
# CONFIG_PKG_igb is not set
# CONFIG_PKG_ixgbe is not set
# CONFIG_PKG_netxtreme2 is not set
# CONFIG_PKG_tg3 is not set

#
# Python2.6
#
CONFIG_PKG_python=y
CONFIG_USE_PYTHON_DEB_PKG=y
CONFIG_PYTHON_DEB_URL="http://security.ubuntu.com/ubuntu/pool/main/p/python2.6/"

#
# Simulation
#
CONFIG_SIMULATION=y
CONFIG_PKG_qemu=y
CONFIG_GIT_QEMU_URL="ssh://${GIT_USER}@gpssh.corp.emc.com:30000/qemu"
# CONFIG_PKG_novnc is not set

#
# Utilities
#
CONFIG_PKG_pciutils=y
CONFIG_PKG_module_init_tools=y
CONFIG_PKG_ethtool=y
CONFIG_PKG_ipmitool=y
CONFIG_PKG_openipmi=y
CONFIG_PKG_kexec=y
CONFIG_PKG_dmidecode=y
CONFIG_PKG_lscpu=y
# CONFIG_PKG_sg3_utils is not set
# CONFIG_PKG_hdparm is not set
CONFIG_PKG_i2c_tools=y
